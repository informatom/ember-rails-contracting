<td>
  {{#link-to 'consumableItems' item.id}}
    <span class="glyphicon glyphicon-zoom-in">{{item.position}}
  {{/link-to}}
</td>
<td>{{input type="text" value=item.term size=1}}</td>
<td>{{view Contracting.DatePickerField value=item.startdate}}</td>
<td>{{formatDate item.enddate}}</td>
<td>{{input type="text" value=item.productNumber}}</td>
<td>{{input type="text" value=item.descriptionDe}}</td>
<td>{{input type="text" value=item.descriptionEn}}</td>
<td>{{input type="text" value=item.amount size=3}} {{input type="text" value=item.unit size=3}}</td>
<td>{{input type="text" value=item.marge size=1}}</td>
<td>{{input type="text" value=item.volumeBw size=4}}</td>
<td>{{input type="text" value=item.volumeColor  size=4}}</td>
<td>{{twoDigits item.price}}</td>
<td>{{twoDigits item.monthlyRate}}</td>
<td>{{input type="text" value=item.monitoringRate size=1}}</td>
<td>{{input type="text" value=item.discountAbs size=3}}</td>
<td>{{twoDigits item.value}}</td>
<td>{{input type="text" value=item.vat size=1}}%</td>
<td>{{twoDigits item.valueInclVat}}</td>
<td>{{formatDatetime item.createdAt}}</td>
<td>{{formatDatetime item.updatedAt}}</td>